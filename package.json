{
  "name": "ot-json1",
  "version": "1.0.1",
  "description": "JSON OT type",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "ot-text-unicode": "4"
  },
  "devDependencies": {
    "@types/node": "^14.0.13",
    "mocha": "^10.2.0",
    "ot-fuzzer": "1.3",
    "ot-simple": "^1.0.0",
    "terser": "^4.6.7",
    "typescript": "^3.9.5"
  },
  "scripts": {
    "test": "mocha test/cursor.js test/test.js test/immutable.js",
    "fuzzer": "node test/fuzzer.js",
